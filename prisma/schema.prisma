generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id    String @id @default(uuid())
    ms_id String @unique
    name  String
}

model Comp {
    id          String    @id @default(uuid())
    comp_date   DateTime
    description String
    book_from   DateTime?
    action      String?
}
